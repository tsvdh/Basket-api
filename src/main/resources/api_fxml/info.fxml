<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.stage.Stage?>

<Stage resizable="false" title="Info" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="common.pre_built.popups.InfoController">
    <scene>
        <Scene>
            <AnchorPane prefHeight="300.0" prefWidth="500.0">
            <children>
               <ScrollPane fitToWidth="true" focusTraversable="false" hbarPolicy="NEVER" prefHeight="300.0" prefWidth="500.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                  <content>
                     <VBox fx:id="infoVBox" spacing="10.0">
                        <padding>
                           <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                        </padding>
                     </VBox>
                  </content>
               </ScrollPane>
            </children></AnchorPane>
        </Scene>
    </scene>
</Stage>
